@model IEnumerable<MyEShop01.Models.KetQuaTimKiemVM>

<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.MaHh)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.TenHh)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Hinh)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.DonGia)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.NgaySX)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Loai)
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.MaHh</td>
				<td>@item.TenHh</td>
				<td>
					<img src="~/Hinh/HangHoa/@item.Hinh" height="100" />
				</td>
				<td>
					@item.DonGia.ToString("#,##0.00") đ
				</td>
				<td>
					@item.NgaySX.ToString("dd/MM/yyyy")
				</td>
				<td>@item.Loai</td>
			</tr>
		}
	</tbody>
</table>
